
l = []
s = 0
n = int(input("enter range"))
#x=int(input("enter elements"))
for i in range(n):
    x = int(input("enter elements"))
    l.append(x)
min = l[0]
max = l[n-1]
for i in range(1, n):
    if(l[i] > max):
        max = l[i]
    if(l[i] < min):
        min = l[i]
print("maximum is", max)
print("minimum is", min)
